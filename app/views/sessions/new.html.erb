<h1>Please sign up or sign in:</h1>
    <%= link_to "Sign Up", new_user_path %>
    <br>
    <!-- <form> -->
      <!-- <div class="form-group"> -->
        
    <%= form_for :session, url: login_path do |f| %>
      <%= f.label :username %>
      <%= f.text_field :username %>
      <%= f.label :password %>
      <%= f.password_field :password %>
    <%= f.submit "Login"%>
    <% end %>
